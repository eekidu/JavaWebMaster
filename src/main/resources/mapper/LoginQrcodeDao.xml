<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kee.db.dao.LoginQrcodeDao">

    <insert id="insert">
        INSERT INTO login_qrcode (qrcode) VALUE (#{qrcode});
    </insert>
    <select id="selectLoginQrcode" resultType="com.kee.db.model.LoginQrcode">
        SELECT *
        FROM login_qrcode
        WHERE qrcode = #{qrcode};
    </select>
    <update id="update">
        UPDATE login_qrcode
        SET state = #{state}
        WHERE qrcode = #{qrcode};
    </update>

</mapper>